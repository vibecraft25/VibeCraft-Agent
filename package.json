{
  "name": "vibecraft-agent",
  "version": "1.0.0",
  "description": "SQLite 데이터베이스를 기반으로 Gemini CLI를 활용하여 React 데이터 시각화 애플리케이션을 자동 생성하는 CLI 도구",
  "main": "dist/cli.js",
  "bin": {
    "vibecraft-agent": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --exec ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "jest --config jest.performance.config.js",
    "test:e2e": "npm run build && jest --config jest.integration.config.js tests/e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "cli",
    "data-visualization",
    "react",
    "sqlite",
    "gemini",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^11.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.536.0",
    "ora": "^8.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.0",
    "sql.js": "^1.13.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^2.1.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/sql.js": "^1.4.9",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  }
}
